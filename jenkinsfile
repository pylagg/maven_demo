pipeline{
	agent any
	    stages {
        	stage("Code Checkout") {
                     steps {
                	git branch: 'master',
                	url: 'https://github.com/pylagg/maven_demo.git'
                  }
              }
		stage('Compile Stage')
		{
			steps{
				withMaven(maven : 'maven3.6'){
				 bat 'mvn clean compile'
				}
			}
		}
		stage('Testing Stage')
		{
			steps{
				withMaven(maven : 'maven3.6'){
				 bat 'mvn test'
				}
			}
		}
		stage('Build Stage')
		{
			steps{
				withMaven(maven : 'maven3.6'){
				 bat 'mvn package'
				}
			}
		}
		
         	
	}
}
